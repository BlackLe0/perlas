#print "0:$separador[0]\n,1:$separador[1]\n,2:$separador[2],\n3:$separador[3]";
#print "$separador[2]";
@separador2=split("score",$separador[2]);
#print "0:$separador2[0]\n";

$prediccion=$separador2[0];
$prediccion=~s/z//;
$prediccion=~s/\-//;
$prediccion=~s/\s//g;
my $bool; 
my $markov=$TIPO{$key};
if ($prediccion eq "PROMISCUOUS") {$bool=1;} else {$bool=0;}
if ($markov eq "PriA") {$markov=1;} 
if ($markov eq "HisA") {$markov=0;} 
if ($markov eq "subHisA") {$markov=0;} 
if ($markov eq "TZ") {$markov=2;} 
my $coincidencia;
if ($markov == $prediccion) {$coincidencia=1;} else {$coincidencia=0;}
print "KEY\t$key\tCARBONELL\t$prediccion\tBOOL\t$bool\tMARKOV\t$markov:$TIPO{$key}\tCOINCIDENCIA\t$coincidencia\n";
}
